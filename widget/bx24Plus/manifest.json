{
  "manifest_version": 2,
  "content_scripts": [
    {
      "js": [
        "bx24Plus.user.js"
      ],
      "matches": [
        "https://*.bitrix24.ru/*",
        "https://*.bitrix24.kz/*"
      ],
      "all_frames": true,
      "run_at": "document_end"
    }
  ],
  "description": "Система расширений для CRM Bitrix24",
  "name": "Bx24Plus",
  "permissions": [
    "activeTab"
  ],
  "version": "1.0.0"
}