{
  "background": {
    "page": "background.html"
  },
  "manifest_version": 2,
  "content_scripts": [
    {
      "js": [
        "bx24Plus.user.js"
      ],
      "matches": [
        "https://*.bitrix24.ru/*"
      ],
      "all_frames": true,
      "run_at": "document_end"
    }
  ],
  "description": "Система расширений для CRM Bitrix24",
  "name": "Bx24Plus",
  "permissions": [
    "http://*/*",
    "unlimitedStorage"
  ],
  "version": "1.0.0"
}